// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Councelors{
  id Int @id @default(autoincrement())
  name String
  email String @unique
  phone String
  college String
  students Patient[]
  
}
model Patient{
  id Int  @unique
  name String
  phone String
  refNumber String
  description String
  residence String
  college String
  deparment String
  counceler Councelors @relation(fields: [councelor], references: [email])
  councelor String
}

